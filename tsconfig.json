{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/components/icons/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/lib/*":[
        "src/lib/*"
      ],
      "@/utils/*":[
        "src/components/utils/*"
      ],
    },
  }
}